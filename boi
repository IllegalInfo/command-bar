		for i, x in pairs(c:GetChildren()) do
		x.Position = x.Position + UDim2.new(0, 0, 0, -15)
		end
		  b.Parent = c
		  b.Text = v.." "
				  b.TextStrokeTransparency = 0.1
		  b.TextTransparency = .1
					  b.TextColor3 = Color3.fromRGB(0, 0, 0)
				  while true do
				   for a = 1,360,1 do wait(.01)
					b.TextStrokeColor3 = Color3.fromHSV(a/360,1,1)
				   end
				   break
				   end
				   for y = 0, 1, 0.1 do
			wait(0.1)
			b.TextStrokeTransparency = 1
			b.TextTransparency = y
			end
  end)
  end
  
		  

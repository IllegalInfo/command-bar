function ParseMessage(Message)
	local Arguments = {}
	Message = string.lower(Message)
	local PrefixMatch = string.match(Message, "^"..PREFIX)
	if PrefixMatch then
		Message = string.gsub(Message, PrefixMatch, "", 1)
		for Argument in string.gmatch(Message, "[^%s]+") do
			table.insert(Arguments, Argument)
		end
	end
	return Arguments
end

function Zanexx(commandname, cmddesc, sc)
	local cmdinformation = {
		commandname
	}
	if sc == nil then
		sc = cmddesc
		cmddesc = nil
		table.insert(cmdinformation, sc)
	else
		table.insert(cmdinformation, cmddesc)
		table.insert(cmdinformation, sc)
	end
	table.insert(ZANEXX.Commands, cmdinformation)
end
game.Players.LocalPlayer.Chatted:Connect(function(msg)
	local IsACmd = false
	msg:lower()
	if msg:sub(1, 3) == "/e " then
		msg = msg:sub(4)
	end
	local args = ParseMessage(msg)
	for i, cmd in pairs(ZANEXX.Commands) do
		if args[1] == cmd[1] then
			IsACmd = true
			table.remove(args, 1)
			if type(cmd[3]) == "function" then
				cmd[3](args)
			else
				cmd[2](args)
			end
		end
	end
	if not IsACmd then
		local x = args[1]
		SendM("[ZANEXX LCM]: "..args[1].." Is not a command")
	end
end)

Bar.Visible = false
local UIS = game:GetService("UserInputService")
local cntrl = false
local visible = false

UIS.InputBegan:Connect(function(input,proc)
		if not proc then
			if input.KeyCode.Name == "Semicolon" then
				visible = true
				Bar.Visible = true
				MainBox.Text = ""
				wait()
				MainBox:CaptureFocus()
			end
		end
	end)

UIS.InputEnded:Connect(function(input)
	if input.KeyCode.Name == "LeftAlt" then
		cntrl = false
	end
end)

MainBox.FocusLost:Connect(function(w)
	local IsACmd = false
	if w == true then
		Bar.Visible = false
		local text = MainBox.Text:lower()
		if text ~= "" or text ~= " " or text ~= nil then
			if text:sub(1, 1) ~= PREFIX then
				text = PREFIX..text
			end
			local args = ParseMessage(text)
			for i, cmd in pairs(ZANEXX.Commands) do
				if args[1] == cmd[1] then
					IsACmd = true
					table.remove(args, 1)
					if type(cmd[3]) == "function" then
						cmd[3](args)
					else
						cmd[2](args)
					end
				end
			end
			if not IsACmd then
				SendM("[ZANEXX LCM]: "..args[1].." Is not a command")
			end
		end
	end
end)


function findplr(name)
local players = {}

if name == 'me' then
table.insert(players, game.Players.LocalPlayer)

elseif name == 'others' then
for i,v in pairs(game:GetService('Players'):GetChildren()) do
    if v.Name ~= game.Players.LocalPlayer.Name then
        table.insert(players, v)
    end
end

elseif name == 'all' then
for i,v in pairs(game:GetService('Players'):GetChildren()) do
    table.insert(players, v)
end

else
local names = game.Players:GetChildren()
for i,v in pairs(names) do
local strlower = string.lower(v.Name)
local sub = string.sub(strlower,1,#name)
if name == sub then 
local player = v
table.insert(players, v)
end
end
end
if players then
return players
else 
SendM("[ZANEXX LCM]: Couldn't find [plr]")
end
end


function teleport(position)
	position = position + Vector3.new(0, 3, 0)
	local BP = Instance.new("BodyPosition")
	BP.MaxForce = Vector3.new(10000, 10000, 10000)
	BP.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
	BP.Position = position
	for i, v in pairs(parts) do
		v.CanCollide = false
	end
	repeat
		wait()
	until (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - position).Magnitude <= 10
	game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(position)
	if BP then
		BP:Destroy()
	end
	for i, v in pairs(parts) do
		v.CanCollide = true
	end
end


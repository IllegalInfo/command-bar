	Zanexx("gay", "Shows A Random Gay% On The Chosen Player(-s)", function(args)
	local target = findplr(args[1])
	
	for i,v in pairs(target) do
	local rnd = math.random(22, 101)
	local pn = v.Name
	if pn == "exclusiveclique" then 
	game.Players:Chat("[ZANEXX V2]: exclusiveclique is 0% gay")
	else
	game.Players:Chat("[ZANEXX V2]: "..pn.." Is "..rnd.."% gay")
	end
	end
	end)
	

local function UDJWXHL_fake_script() -- TextBox.LocalScript 
	local script = Instance.new('LocalScript', MainBox)

	local color1 = Color3.fromRGB(255, 255, 255)
	local color2 = Color3.fromRGB(255, 94, 226) 
	local Players = game.Players
	local changeSpeed = 10
	spawn(function()
				  local i = -1
				  while true do
					  for i=-1,1,1/changeSpeed do
						  local f = math.abs(i)
						  TextBox.TextStrokeColor3 = Color3.fromRGB(
							  255 * (color1.r + (color2.r - color1.r) * f),
							  255 * (color1.g + (color2.g - color1.g) * f), 
							  255 * (color1.b + (color2.b - color1.b) * f))
						  wait(0.05)
		end
		end
	end)
	
end

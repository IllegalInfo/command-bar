Zanexx("prefix", "Lets The LocalPlayer Change The Prefix To Anything ( !, @, #, $...)", function(args)
	SendM("[ZANEXX LCM]: Prefix Before: "..PREFIX.."")
	  local pargs = args[1]
	  PREFIX = pargs
	  SendM("[ZANEXX LCM]: Prefix Changed To: "..PREFIX.."")
	  
	  if PREFIX == "!" then
	  SendM("[ZANEXX LCM]: Prefix Set To [!] (Matts Admin)")
	  
	  elseif PREFIX == "." then
	  SendM("[ZANEXX LCM]: Prefix Set To [.] (Faxng Admin)")
	  
	  elseif PREFIX == ":" then
	  SendM("[ZANEXX LCM]: Prefix Set To [:] (Aidez)")

	  elseif PREFIX == ";" then
	  SendM("[ZANEXX LCM]: Prefix Set To [;] (DrPoppa's Admin)")

	  elseif PREFIX == "/" then
	  SendM("[ZANEXX LCM]: Prefix Set To [/] (Terminal X)")

	  elseif PREFIX == "-" then
	  SendM("[ZANEXX LCM]: Prefix Set To [-] (Fates Admin)")

	  elseif PREFIX == "+" then
	  SendM("[ZANEXX LCM]: Prefix Set To [+] (Clouds Admin)")
	
	  elseif PREFIX == "'" then
	  SendM("[ZANEXX LCM]: Prefix Set To ['] (q5ep's Admin)")

	  elseif PREFIX == "]" then
	  SendM("[ZANEXX LCM]: Prefix Set To []] (ZANEXX)")
	  
	  elseif PREFIX == "none" then
		SendM("[ZANEXX LCM]: Prefix Set To [None]")
		PREFIX = ""
	  end
	  end)



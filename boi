		local pn = v.Name
		SendM("[ZANEXX LCM]: Loop-Healing "..pn.."")
		local num = args[2]
		while wait() do
		game.ReplicatedStorage.Event:FireServer("TPD", -num, v.Character.Humanoid)
		if v.Character.Humanoid.Health == 100 then
		break
		end
		end
		end
		end)
	

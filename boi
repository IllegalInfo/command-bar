	Zanexx("annoy", "Annoys The Chosen Player", function(args)
	local plrs = game:GetService("Players")
	local grav = Instance.new("RocketPropulsion")
	local target = findplr(args[1])

	for i,v in pairs(target) do
	local pn = v.Name
	SendM("[ZANEXX LCM]: Annoying "..pn..".")
	
	local noclip = true 
	char = game.Players.LocalPlayer.Character
	while true do
	if noclip == true then 
	for _,v in pairs(char:children()) do 
	pcall(function() 
	if v.className == "Part" then v.CanCollide = false
	elseif v.ClassName == "Model" then 
	v.Head.CanCollide = false
	end 
	end)
	end 
	end 
	game:service("RunService").Stepped:wait() 
	end

	grav.ThrustP = 5
    grav.MaxSpeed = 2000000
	grav.MaxThrust = 1800000
	grav:fire()

	grav.Parent = plrs.LocalPlayer.Character.Torso
	grav.Target = v.Character.Torso
	end
	end)


	Zanexx("nc", "Enables Noclip For The LocalPlayer Only", function(args)
		local noclip = true 
		char = game.Players.LocalPlayer.Character
		while true do
		if noclip == true then 
		for _,v in pairs(char:children()) do 
		pcall(function() 
		if v.className == "Part" then v.CanCollide = false
		elseif v.ClassName == "Model" then 
		v.Head.CanCollide = false
		end 
		end)
		end 
		end 
		game:service("RunService").Stepped:wait() 
		end
		SendM("[ZANEXX LCM]: Noclip Enabled, Say [cl] To Disable")
		end)

	-----------------------------------------------------------------------------------------------------------------

	Zanexx("cl", "Disables Noclip", function(args)
		local noclip = false
		char = game.Players.LocalPlayer.Character
		while true do
		if noclip == false then 
		for _,v in pairs(char:children()) do 
		pcall(function() 
		if v.className == "Part" then v.CanCollide = true
		elseif v.ClassName == "Model" then 
		v.Head.CanCollide = true
		end 
		end)
		end 
		end 
		game:service("RunService").Stepped:wait()
		end
		SendM("[ZANEXX LCM]: Noclip Disabled")
		end) 

